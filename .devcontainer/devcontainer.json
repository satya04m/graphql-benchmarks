{
  "name": "tc-benchmarks Codespace",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "runArgs": [
    "--privileged",
    "--cap-add=SYS_ADMIN",
    "-v", "/var/run/docker.sock:/var/run/docker.sock"
  ],
  "forwardPorts": [8000, 3000],
  "postCreateCommand": "bash ./setup.sh"
}
